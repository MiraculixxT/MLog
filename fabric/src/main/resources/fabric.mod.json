{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${author}"
  ],
  "license": "${license}",
  "icon": "assets/mlog/icon.png",
  "contact": {
    "homepage": "https://modrinth.com/mod/${modrinth}",
    "issues": "https://dc.mutils.net"
  },
  "environment": "${environment}",
  "entrypoints": {
    "server": [
      "de.miraculixx.mlog.server.MLogServer"
    ],
    "client": [
      "de.miraculixx.mlog.client.MLogClient"
    ]
  },
  "mixins": [
    "mlog.mixins.json"
  ],
  "suggests": {
    "modmenu":"*"
  },
  "depends": {
    "fabricloader": ">=0.15.0",
    "minecraft": "${mcversion}",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": "*"
  }
}
